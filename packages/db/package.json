{
  "name": "db",
  "version": "1.0.0",
  "packageManager": "pnpm@10.13.1",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_ENV=prod tsup --config ./tsup-config.js",
    "dev": "cross-env NODE_ENV=dev tsup --config ./tsup-config.js",
    "typeorm": "typeorm-ts-node-esm",
    "m:g": "pnpm typeorm migration:generate",
    "m:r": "pnpm typeorm migration:run",
    "m:c": "pnpm typeorm migration:create",
    "m:rev": "pnpm typeorm migration:revert"
  },
  "exports": {
    ".": {
      "import": "./build/index.js",
      "types": "./build/index.d.ts"
    }
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "ts-node": "^10.9.2"
  }
}
